## included file /home/brian/.ctwm/defaults
## Function to reconfigure the window manager
Function "reconfig" { f.exec "/usr/share/ctwm/config ctwm 3.8.1" f.restart }
## included file /home/brian/.ctwm/buttons
#
# Define some useful functions for motion-based actions.
#
Function "move-or-lower" { f.move f.deltastop f.lower }
Function "move-or-raise" { f.move f.deltastop f.raise }
Function "move-or-iconify" { f.move f.deltastop f.iconify }
## included file /home/brian/.ctwm/keys
Function "next-cycle-focus" { f.forwiconmgr f.raise }
Function "prev-cycle-focus" { f.backiconmgr f.raise }
Function "set-system-style-Airforce" { f.exec "/home/brian/.ctwm/setstyle ctwm /usr/share/ctwm/styles/Airforce" f.function "reconfig" }
Function "set-system-style-Archlinux" { f.exec "/home/brian/.ctwm/setstyle ctwm /usr/share/ctwm/styles/Archlinux" f.function "reconfig" }
Function "set-system-style-Atc" { f.exec "/home/brian/.ctwm/setstyle ctwm /usr/share/ctwm/styles/Atc" f.function "reconfig" }
Function "set-system-style-Aviation" { f.exec "/home/brian/.ctwm/setstyle ctwm /usr/share/ctwm/styles/Aviation" f.function "reconfig" }
Function "set-system-style-Blackbirds" { f.exec "/home/brian/.ctwm/setstyle ctwm /usr/share/ctwm/styles/Blackbirds" f.function "reconfig" }
Function "set-system-style-Circuits" { f.exec "/home/brian/.ctwm/setstyle ctwm /usr/share/ctwm/styles/Circuits" f.function "reconfig" }
Function "set-system-style-Classic" { f.exec "/home/brian/.ctwm/setstyle ctwm /usr/share/ctwm/styles/Classic" f.function "reconfig" }
