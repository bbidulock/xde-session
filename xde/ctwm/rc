Function "set-system-style-Pedestals" { f.exec "/home/brian/.ctwm/setstyle ctwm /usr/share/ctwm/styles/Pedestals" f.function "reconfig" }
Function "set-system-style-Penguins" { f.exec "/home/brian/.ctwm/setstyle ctwm /usr/share/ctwm/styles/Penguins" f.function "reconfig" }
