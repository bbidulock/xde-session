    "TWM"                             f.exec "exec twm &"
    "VTWM"                            f.exec "exec vtwm &"
    "Waimea"                          f.exec "exec waimea &"
    "WindowMaker"                     f.exec "exec wmaker &"
    "windwm"                          f.exec "exec wind &"
    "Xfce Session"                    f.exec "exec startxfce4 &"
    "yeahwm"                          f.exec "exec yeahwm &"
    ""                                f.separator
    "Restart"                         f.restart
    "Quit"                            f.quit
}

