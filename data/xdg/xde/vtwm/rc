## Fine tune font offsets.
TextOffsets {
	TitleFont	    "+1+1"
	MenuTitleFont	    "+2+1"
	MenuFont	    "+1+1"
	IconManagerFont	    "+0+0"
	VirtualDesktopFont  "-1-4"
}


