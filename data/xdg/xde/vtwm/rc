## included file /home/brian/.vtwm/defaults
## Function to reconfigure the window manager
Function "reconfig" { f.exec "/usr/share/vtwm/config vtwm 5.5.0" f.restart }
## included file /home/brian/.vtwm/buttons
#
# Define some useful functions for motion-based actions.
#
Function "move-or-lower" { f.move f.deltastop f.lower }
Function "move-or-raise" { f.move f.deltastop f.raise }
Function "move-or-iconify" { f.move f.deltastop f.iconify }
## included file /home/brian/.vtwm/keys
Function "next-cycle-focus" { f.forwiconmgr f.raise }
Function "prev-cycle-focus" { f.backiconmgr f.raise }
Function "set-system-style-Airforce" { f.exec "/home/brian/.vtwm/setstyle vtwm /usr/share/vtwm/styles/Airforce" f.function "reconfig" }
Function "set-system-style-Archlinux" { f.exec "/home/brian/.vtwm/setstyle vtwm /usr/share/vtwm/styles/Archlinux" f.function "reconfig" }
Function "set-system-style-Atc" { f.exec "/home/brian/.vtwm/setstyle vtwm /usr/share/vtwm/styles/Atc" f.function "reconfig" }
Function "set-system-style-Aviation" { f.exec "/home/brian/.vtwm/setstyle vtwm /usr/share/vtwm/styles/Aviation" f.function "reconfig" }
Function "set-system-style-Blackbirds" { f.exec "/home/brian/.vtwm/setstyle vtwm /usr/share/vtwm/styles/Blackbirds" f.function "reconfig" }
Function "set-system-style-Circuits" { f.exec "/home/brian/.vtwm/setstyle vtwm /usr/share/vtwm/styles/Circuits" f.function "reconfig" }
Function "set-system-style-Classic" { f.exec "/home/brian/.vtwm/setstyle vtwm /usr/share/vtwm/styles/Classic" f.function "reconfig" }
Function "set-system-style-Default" { f.exec "/home/brian/.vtwm/setstyle vtwm /usr/share/vtwm/styles/Default" f.function "reconfig" }
Function "set-system-style-Dishes" { f.exec "/home/brian/.vtwm/setstyle vtwm /usr/share/vtwm/styles/Dishes" f.function "reconfig" }
Function "set-system-style-Pedestals" { f.exec "/home/brian/.vtwm/setstyle vtwm /usr/share/vtwm/styles/Pedestals" f.function "reconfig" }
